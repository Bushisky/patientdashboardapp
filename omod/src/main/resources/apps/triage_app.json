[
    {
        "id": "patientdashboardapp.triage",        // unique app id, (independent of the template)
        "instanceOf": "patientqueue.template.viewQueue",        // unique id of the template to inherit
        "label": "Triage",        // text or message code
        "description": "View patient in OPD queue",        // text or message code
        "extensions": [        // extensions that attach to extension points of _other_ apps
            {
                "id": "patientdashboardapp.opdqueue.homepageLink",        // id for this extension, must be unique across all apps
                "extensionPointId": "org.openmrs.referenceapplication.homepageLink",
                "type": "link",
                "label": "Triage",
                "url": "patientqueueui/queue.page?app=patientdashboardapp.triage",
                "icon": "icon-list-ol",
                "order": 2,
                "requiredPrivilege": "App: referenceapplication.quickSummary" //patientdashboardapp.opdqueue
            }
        ],
        "config": {
            "onSelectUrl": "/patientdashboardapp/triage.page?patientId={{patientId}}&opdId={{opdId}}&queueId={{queueId}&visitStatus={{visitStatus}}",
            "title": "Triage Queue"
        }

    }
] 